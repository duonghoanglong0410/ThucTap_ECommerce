<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Blueberry - Multi Purpose eCommerce Template.">
    <meta name="keywords"
          content="eCommerce, mart, apparel, bootstrap 5, catalog, fashion, html, multipurpose, online store, shop, store, template">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"] - TT_ECommerce</title>

    <!--Favicon-->
    <link rel="icon" href="@Url.Content("~/user/favicon/favicon.png")" type="image/x-icon">

    <!-- Css All Plugins Files -->
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/remixicon.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/aos.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/swiper-bundle.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/owl.carousel.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/slick.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/animate.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/vendor/jquery-range-ui.css")">

    <!-- Main Style -->
    <link rel="stylesheet" href="@Url.Content("~/user/assets/css/style.css")">

</head>
<body>

    <!-- Loader -->
    <div class="bb-loader">
        <img src="~/user/assets/img/logo/loader.png" alt="loader">
        <span class="loader"></span>
    </div>



    @Html.Partial("~/Views/Shared/Partials/_Header.cshtml")


    <!--Category
    Html.Partial("~/Views/Shared/Partials/_Category.cshtml")
    -->
    @RenderBody()

    <!--Cart Siderbar
    Html.Partial("~/Views/Shared/Partials/_CartSidebar.cshtml")
    -->

    <!--Category Popup-->
    @Html.Partial("~/Views/Shared/Partials/_CategoryPopup.cshtml")

    <!--Quick View Modal-->
    @Html.Partial("~/Views/Shared/Partials/_QuickViewModal.cshtml")

    <!--News Letter-->

    <!--Tool Sidebar-->


   
    <!--Footer-->
    @Html.Partial("~/Views/Shared/Partials/_Footer.cshtml")
    <!-- Back to top  -->
    <a href="#Top" class="back-to-top result-placeholder">
        <i class="ri-arrow-up-line"></i>
        <div class="back-to-top-wrap active-progress">
            <svg viewBox="-1 -1 102 102">
                <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98"></path>
            </svg>
        </div>
    </a>

    <!-- Plugins -->
    <script src="@Url.Content("~/user/assets/js/vendor/bootstrap.bundle.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/jquery.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/jquery.zoom.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/aos.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/swiper-bundle.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/smoothscroll.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/countdownTimer.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/owl.carousel.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/slick.min.js")"></script>
    <script src="@Url.Content("~/user/assets/js/vendor/jquery-range-ui.min.js")"></script>

    <!-- main-js -->
    <script src="@Url.Content("~/user/assets/js/main.js")"></script>

    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
